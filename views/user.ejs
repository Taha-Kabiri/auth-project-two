<html>
  <head>
    <link rel="stylesheet" href="/css/style.css">
    <title><%= title %></title>
  </head>
  <body>
    <table>
      <thead>
        <tr>
          <th>id</th>
          <th>firstname</th>
          <th>email</th>
          <th>password</th>
        </tr>
      </thead>
      <tbody>
            <%  users.forEach(user=>{ %>
        <tr>  
          <td><%= user.id %></td>
          <td><%= user.firstname %></td>
          <td><%= user.email %></td>
          <td><%= user.password%></td>
        </tr>

             <% }) %>

      </tbody>
    </table>
  </body>
</html>